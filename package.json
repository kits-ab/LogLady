{
  "name": "loglady",
  "version": "1.0.0",
  "description": "Log file viewer",
  "author": "lalala",
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "concurrently": "^4.1.0",
    "electron-is-dev": "^1.0.1",
    "eslint-config-react-app": "^3.0.7",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "lint": "eslint src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-pre-commit": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build -l -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kits-ab/LogLady.git"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test-pre-commit"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "Utility",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "Development",
      "target": "AppImage"
    },
    "directories": {
      "buildResources": "assets"
    }
  }
}
