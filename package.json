{
  "name": "loglady",
  "version": "v1.0.7",
  "description": "Log file viewer",
  "author": "lalala",
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "always-tail": "^0.2.0",
    "chokidar": "^2.1.2",
    "concurrently": "^4.1.0",
    "electron-is-dev": "^1.0.1",
    "electron-simple-updater": "^1.4.3",
    "eslint-config-react-app": "^3.0.7",
    "md5": "^2.2.1",
    "node-notifier": "^5.4.0",
    "nthline": "^1.0.1",
    "react": "^16.8.2",
    "react-color": "^2.17.0",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5",
    "react-test-renderer": "^16.8.4",
    "read-last-lines": "^1.6.0",
    "styled-components": "^4.1.3",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "electron": "^4.0.5",
    "electron-builder": "^20.38.5",
    "eslint": "5.12.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "lint": "eslint src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "build -lmw -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kits-ab/LogLady.git"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test -- --coverage"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "src/**/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "Utility",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "Development",
      "target": "AppImage"
    },
    "directories": {
      "buildResources": "assets"
    }
  }
}
