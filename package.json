{
  "name": "loglady",
  "version": "v1.1.3",
  "description": "Log file viewer",
  "author": "lalala",
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "chokidar": "^3.0.2",
    "concurrently": "^4.1.2",
    "electron-is-dev": "^1.1.0",
    "electron-settings": "^3.2.0",
    "electron-simple-updater": "^1.5.0",
    "eslint-config-react-app": "^5.0.1",
    "fs-backwards-stream": "^1.0.0",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "memoize-one": "^5.1.1",
    "node-notifier": "^5.4.3",
    "nthline": "^1.0.1",
    "react": "^16.9.0",
    "react-color": "^2.17.3",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-scripts": "^3.1.1",
    "react-split-pane": "^0.1.87",
    "react-test-renderer": "^16.9.0",
    "react-tooltip": "^3.11.1",
    "react-window": "^1.8.5",
    "read-last-lines": "^1.7.1",
    "redux": "^4.0.4",
    "styled-components": "^4.3.2",
    "wait-on": "^3.3.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "electron": "^6.0.8",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "lint": "eslint src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron --inspect .\"",
    "electron-pack": "electron-builder -lmw",
    "preelectron-pack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kits-ab/LogLady.git"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && CI=true npm run test -- --coverage"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "src/**/*",
      "build/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "Utility",
      "target": "dmg",
      "icon": "./public/icons/mac/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "./public/icons/win/icon.ico"
    },
    "linux": {
      "category": "Development",
      "target": "AppImage",
      "icon": "./public/icons/png"
    },
    "directories": {
      "buildResources": "assets"
    }
  }
}
